###      Do not modify by hand       ###
########################################

[agent]
  interval = "10s"
  round_interval = true
  metric_batch_size = 1000
  metric_buffer_limit = 10000
  collection_jitter = "0s"
  flush_interval = "10s"
  flush_jitter = "0s"
  precision = ""
  debug = false
  quiet = false
  logfile = ""
  hostname = "raspberrypi-1"
  omit_hostname = false

####################################
#          OUTPUT PLUGINS          #
####################################

[[outputs.influxdb_v2]]
  urls = ["http://192.168.1.100:8086"]
  token = "{{ influxdb_token }}"
  organization = "Casa de Kempfert"
  bucket = "Metrics"

####################################
#          INPUT PLUGINS           #
####################################

[[inputs.cpu]]
  percpu = true
  totalcpu = true
  collect_cpu_time = false
  report_active = false
[[inputs.disk]]
  ignore_fs = ["tmpfs", "devtmpfs", "devfs", "iso9660", "overlay", "aufs", "squashfs"]
[[inputs.diskio]]
[[inputs.docker]]
  endpoint = "unix:///var/run/docker.sock"
  perdevice = false
  total_include = ["cpu", "blkio", "network"]
  total = true
[[inputs.mem]]
[[inputs.net]]
[[inputs.netstat]]
[[inputs.processes]]
[[inputs.swap]]
[[inputs.system]]
